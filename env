APPLICATION_NAME=${APPLICATION_NAME}
NUXEO_VERSION=${NUXEO_VERSION:-latest}
NUXEO_PORT=${NUXEO_PORT}
NUXEO_PACKAGES=${APPLICATION_NAME} ${NUXEO_PACKAGES}
NUXEO_TEMPLATES=${NUXEO_TEMPLATES}

STUDIO_USERNAME=${STUDIO_USERNAME:-${USER}}
STUDIO_CREDENTIALS=${STUDIO_CREDENTIALS}

NUXEO_HOSTNAME=${NUXEO_HOSTNAME}
NUXEO_MPINSTALL_OPTIONS=${NUXEO_MPINSTALL_OPTIONS:---relax=false}
NUXEO_INSTALL_HOTFIX=${NUXEO_INSTALL_HOTFIX:-true}
NUXEO_DATA=${NUXEO_DATA:-/var/lib/nuxeo/data}
NUXEO_LOG=${NUXEO_LOG:-/var/log/nuxeo}
NUXEO_USER=${NUXEO_USER:-nuxeo}

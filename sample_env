APPLICATION_NAME=${APPLICATION_NAME}
NUXEO_IMAGE=docker.packages.nuxeo.com/nuxeo/nuxeo:latest
NUXEO_PORT=8080
NUXEO_DEV=true
NUXEO_PACKAGES=${APPLICATION_NAME} ${NUXEO_PACKAGES}
NUXEO_TEMPLATES=${NUXEO_TEMPLATES}

INSTALL_RPM=

STUDIO_USERNAME=${STUDIO_USERNAME:-${USER}}
STUDIO_CREDENTIALS=${STUDIO_CREDENTIALS}

ELASTIC_VERSION=7.9.3
MONGO_VERSION=4.4

NUXEO_HOSTNAME=${NUXEO_HOSTNAME}
NUXEO_MPINSTALL_OPTIONS=--relax=false
NUXEO_INSTALL_HOTFIX=true
NUXEO_DATA=/var/lib/nuxeo/data
NUXEO_LOG=/var/log/nuxeo
NUXEO_USER=nuxeo
